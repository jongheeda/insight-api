{
  "network": "testnet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api-zcash",
    "insight-ui-zcash",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/lib/node_modules/bitcore/node_modules/bitcore-node/bin/bitcoind"
      }
    },
    "insight-api-zcash": {
      "routePrefix": "api"
    },
    "insight-ui-zcash": {
      "apiPrefix": "api",
      "routePrefix": ""
    }
  }
}
